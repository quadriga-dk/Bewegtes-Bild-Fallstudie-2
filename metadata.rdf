        <schema1:name>Digital Humanities und Filmwissenschaft</schema1:name>
        <schema1:teaches>Grundzüge der Digital Humanities und deren Rolle in der Filmwissenschaft</schema1:teaches>
        <schema1:timeRequired>PT45M</schema1:timeRequired>
        <schema1:url rdf:resource="https://quadriga-dk.github.io/Bewegtes-Bild-Fallstudie-2/einleitung/toc.html"/>
